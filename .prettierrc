{
  "plugins": [
    "prettier-plugin-organize-attributes",
    "prettier-plugin-organize-imports"
  ],
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 80
}
